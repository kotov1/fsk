- var page = "error"

doctype html
html(lang="ru")

	include pug-modules/head

	body

		.wrapper


			// header
			include pug-modules/header
			// /.header

			
			.page-error
				.container
					.page-error__inner
						.page-error__title 404
						.page-error__message
							p Ой, что то пошло не так :( 
							p Страница не найдена.
						.page-error__btns
							a.btn.btn--transp(href="/") 
								img.svg.btn__ic.ic-home(src="img/icons/ic-home.svg", alt="home", width="20", height="20")
								| Вернуться на главную
							a.btn.btn--bg(href="#") 
								img.svg.btn__ic.ic-arrow(src="img/icons/ic-arrow.svg", alt="link", width="20", height="20")
								| Наши объекты

				// /.container


			// footer
			include pug-modules/footer
			// /.footer
			

		


