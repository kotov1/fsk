							a.contact-link(href="mailto:oks@fsknw.ru") 
								.ic-container
									img.svg.btn__ic(src="img/icons/ic-envelope.svg", alt="email", width="20", height="15")
								| oks@fsknw.ru
							span.contact-link
								.ic-container
									img.svg.btn__ic(src="img/icons/ic-time.svg", alt="geo", width="19", height="19")
