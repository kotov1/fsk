			.card__col-2
				.card__img.js-tab-item
					.card-img-btns
						button.ui-btn.zoom-link(href="img/img-plan-big.jpg")
							img.svg(src="img/icons/ic-zoom.svg", width="15", height="15", alt="zoom")
						a.interactive-btn.interactive-favorite(href="#")
							img.svg(src="img/icons/ic-star.svg", alt="Добавить в закладки")
					img.img(src="img/img-plan.jpg", alt="plan")
